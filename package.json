{
  "name": "lookahead",
  "version": "1.0.0",
  "description": "UoM Timetable Optimiser",
  "main": "dist/server.js",
  "dependencies": {
    "@types/cheerio": "^0.22.14",
    "@types/dotenv": "^6.1.1",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.37",
    "@types/request": "^2.48.3",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^4.1.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^39.2.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "rotating-file-stream": "^1.4.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.12",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "ts-node": "^8.5.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsc": "^1.20150623.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "build-ts": "tsc",
    "start": "node --tls-min-v1.0 dist/server.js ",
    "dev-debug": "concurrently \"npm run express-debug\" \"npm run client\"",
    "express-debug": "node --inspect=5000 -r ts-node/register server.ts",
    "express-debug:watch": "nodemon",
    "server": "npx ts-node-dev --tls-min-v1.0 -- server.ts",
    "test": "npm run build-ts && mocha dist/test --recursive",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev-install": "npm i && npm i --prefix client",
    "postinstall": "npm run build-ts",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trontor/lookahead.git"
  },
  "author": "Rohyl Joshi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Trontor/lookahead/issues"
  },
  "homepage": "https://github.com/Trontor/lookahead#readme",
  "proxy": "http://localhost:5000/"
}
